{
  "manifest_version": 3,
  "name": "Personal Go-links",
  "version": "0.1",
  "description": "Redirects http://go/ links",
  "action": {
    "default_title": "Personal Go-links",
    "default_popup": "popup/dist/index.html"
  },
  "permissions": ["declarativeNetRequest", "tabs", "storage"],
  "host_permissions": ["*://go/*"]
}

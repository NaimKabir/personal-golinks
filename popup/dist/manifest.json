{
  "manifest_version": 3,
  "name": "Personal Shortlinks",
  "version": "1.0",
  "description": "Lets you make custom 'to/' links to sites you want to remember! Like bookmarks that your fingers remember to type ðŸ˜Š.",
  "action": {
    "default_title": "Personal Shortlinks",
    "default_popup": "index.html",
    "default_icon": {
      "16": "assets/icon16.png",
      "24": "assets/icon24.png",
      "32": "assets/icon32.png"
    }
  },
  "permissions": ["declarativeNetRequest", "tabs", "storage"],
  "host_permissions": ["*://go/*", "*://to/*"]
}
